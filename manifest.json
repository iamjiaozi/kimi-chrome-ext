{
  "manifest_version": 3,
  "name": "kimi网页增强助手",
  "version": "0.1.0",
  "description": "在浏览 www.kimi.com 时增强会话体验：收集用户问题并提供快速跳转。",
  "host_permissions": [
    "*://www.kimi.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.kimi.com/*"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_idle"
    }
  ]
}
